::-webkit-scrollbar {
    width: 8px;
  }
  
  /* Track */
  ::-webkit-scrollbar-track {
    box-shadow: inset 0 0 5px dimgrey; 
    border-radius: 0px;
    background:dimgrey;
  }
   
  /* Handle */
  ::-webkit-scrollbar-thumb {
    background:#c3c3c3; 
    border-radius: 5px;
  }
  
  /* Handle on hover */
  ::-webkit-scrollbar-thumb:hover {
    background:#DCDCDC; 
  }
